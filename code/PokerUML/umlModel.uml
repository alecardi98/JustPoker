              <language>JAVA</language>
              <body>clientId = nextId();&#xD;
socket = clientSocket;&#xD;
try {&#xD;
    this.out = new ObjectOutputStream(clientSocket.getOutputStream());&#xD;
    this.in = new ObjectInputStream(clientSocket.getInputStream());&#xD;
} catch (IOException e) {&#xD;
    e.printStackTrace();&#xD;
}&#xD;
// invio clientId&#xD;
sendMessage(new Message(MessageType.CLIENT_ID, clientId));</body>
            </ownedBehavior>
            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tn-hZPEvEfC5cpUsQOGJ5Q" name="run" specification="_tn-hYvEvEfC5cpUsQOGJ5Q">
              <language>JAVA</language>
              <body>try {&#xD;
    Object obj;&#xD;
    while (true) {&#xD;
        obj = in.readObject();&#xD;
        Message msg = (Message) obj;&#xD;
        switch(msg.getType()) {&#xD;
            case PLAYER_JOIN:&#xD;
                handlePlayerJoin((Player) msg.getData());&#xD;
                break;&#xD;
            case QUIT:&#xD;
                handleQuit((int) msg.getData());&#xD;
                cleanup();&#xD;
                return;&#xD;
            default:&#xD;
                System.out.println(&quot;ERRORE! Messaggio sconosciuto.&quot;);&#xD;
                break;&#xD;
        }&#xD;
    }&#xD;
} catch (IOExceptionClassNotFoundException |  e) {&#xD;
    System.out.println(&quot;Errore di comunicazione col client.&quot;);&#xD;
    cleanup();&#xD;
}</body>
            </ownedBehavior>
            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tn_vg_EvEfC5cpUsQOGJ5Q" name="handlePlayerJoin" specification="_tn_vgPEvEfC5cpUsQOGJ5Q">
              <language>JAVA</language>
              <body>Server.getServer().registerPlayer(clientId, player);</body>
