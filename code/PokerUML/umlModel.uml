            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tjseUfEvEfC5cpUsQOGJ5Q" name="run" specification="_tjseUPEvEfC5cpUsQOGJ5Q">
              <language>JAVA</language>
              <body>try {&#xD;
    Object obj;&#xD;
    while ((obj = in.readObject()) != null) {&#xD;
        Message msg = (Message) obj;&#xD;
        // gestione delle risposte del Server&#xD;
        switch(msg.getType()) {&#xD;
            case CLIENT_ID:&#xD;
                client.setClientId((int) msg.getData());&#xD;
                client.sendMessage(new Message(MessageType.PLAYER_JOIN, client.getPlayer()));&#xD;
                break;&#xD;
            case START_GAME:&#xD;
                client.setGame((Game) msg.getData());&#xD;
                client.startToPlay();&#xD;
                break;&#xD;
            case YOUR_TURN:&#xD;
                break;&#xD;
            default:&#xD;
                System.out.println(&quot;ERRORE! Messaggio sconosciuto.\n&quot;);&#xD;
                break;&#xD;
        }&#xD;
    }&#xD;
} catch (IOExceptionClassNotFoundException |  e) {&#xD;
    System.out.println(&quot;Server Disconnesso.&quot;);&#xD;
}</body>
            </ownedBehavior>
