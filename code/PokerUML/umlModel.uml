            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_toAWkfEvEfC5cpUsQOGJ5Q" name="handleQuit" specification="_tn_vhPEvEfC5cpUsQOGJ5Q">
              <language>JAVA</language>
              <body>Server.getServer().removePlayer(clientId);</body>
            </ownedBehavior>
            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_toA9oPEvEfC5cpUsQOGJ5Q" name="sendMessage" specification="_toAWkvEvEfC5cpUsQOGJ5Q">
              <language>JAVA</language>
              <body>try {&#xD;
    out.writeObject(msg);&#xD;
    out.flush();&#xD;
} catch (IOException e) {&#xD;
    e.printStackTrace();&#xD;
}</body>
            </ownedBehavior>
            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_toA9o_EvEfC5cpUsQOGJ5Q" name="cleanup" specification="_toA9ofEvEfC5cpUsQOGJ5Q">
              <language>JAVA</language>
              <body>try {&#xD;
    in.close();&#xD;
    out.close();&#xD;
    socket.close();&#xD;
} catch (IOException e) {&#xD;
    System.out.println(&quot;BHO&quot;);&#xD;
}&#xD;
System.out.println(&quot;ClientHandler rimosso.&quot;);&#xD;
Server.getServer().removeClient(this);</body>
